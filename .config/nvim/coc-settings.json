{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "tsserver.enable": true,
  "tsserver.formatOnType": true,
  "coc.preferences.formatOnSaveFiletypes": ["css", "Markdown", "js", "jsx", "ts", "tsx", "go"]
}
